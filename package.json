{
  "name": "react-mobx-nimble",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Todo app w/React, Mobx and Nimble",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=dev npm run webpack-dev-server",
    "build:stag": "cross-env NODE_ENV=staging npm run webpack-build",
    "build:prod": "cross-env NODE_ENV=production npm run webpack-build",
    "webpack-build": "rimraf dist && webpack --config config/webpack.build.js --progress --profile --bail",
    "webpack-dev-server": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base client/",
    "_start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
    "_build": "webpack -p --progress --colors"
  },
  "main": "index.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/node": "^8.0.46",
    "@types/react": "^16.0.16",
    "@types/react-dom": "^16.0.2",
    "@types/webpack": "^3.0.13",
    "awesome-typescript-loader": "^3.2.3",
    "copy-webpack-plugin": "^4.2.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "mobx-logger": "^0.6.0",
    "mobx-react-devtools": "^4.2.15",
    "node-sass": "^4.6.1",
    "postcss": "^6.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-reporter": "^5.0.0",
    "react-hot-loader": "^3.1.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "to-string-loader": "^1.1.5",
    "typescript": "3.8.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1"
  }
}
